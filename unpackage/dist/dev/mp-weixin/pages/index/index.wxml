<view class="container"><view data-event-opts="{{[['tap',[['bindViewTap',['$event']]]]]}}" class="userinfo" bindtap="__e"><image class="userinfo-avatar" src="{{userInfo.avatarUrl}}" background-size="cover"></image></view><view><text class="userinfo-nickname">{{userInfo.nickName+"（Dunizb）"}}</text></view><view class="usermotto"><button type="default" size="{{primarySize}}" plain="{{plain}}" hover-class="button-hover" disabled="{{disabled}}" data-event-opts="{{[['tap',[['toCalc',['$event']]]]]}}" bindtap="__e">{{motto}}</button></view><view><navigator class="github" url="view" hover-class="navigator-hover"><icon class="icon" type="{{iconType}}" size="20"></icon>GitHub</navigator></view></view>